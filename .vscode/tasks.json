{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build rust explore",
            "type": "shell",
            "command": "cargo",
            "args": [
                "build"
            ],
            "group": "build",
            "options": {
                "cwd": "${workspaceFolder}/rust/explore"
            },
            "problemMatcher": []
        },
        {
            "label": "run rust explore",
            "type": "shell",
            "command": "cargo",
            "args": [
                "run"
            ],
            "group": {
                "kind": "build",
                "isDefault": false,
            },
            "options": {
                "cwd": "${workspaceFolder}/rust/explore"
            },
            "problemMatcher": []
        }
    ]
}